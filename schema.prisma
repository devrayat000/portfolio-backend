// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model Admin {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Project {
  id          String    @id @default(cuid())
  title       String    @default("")
  demo        String    @default("")
  description Json      @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  source      String    @default("")
  tags        Tag[]     @relation("Project_tags")
  images      Image[]   @relation("Project_images")
  createdAt   DateTime? @default(now())
}

model Image {
  id                  String    @id @default(cuid())
  label               String    @default("")
  image_filesize      Int?
  image_extension     String?
  image_width         Int?
  image_height        Int?
  image_id            String?
  createdAt           DateTime? @default(now())
  from_Project_images Project[] @relation("Project_images")
}

model Tag {
  id       String    @id @default(cuid())
  name     String    @default("")
  projects Project[] @relation("Project_tags")
}

model ApiKey {
  id        String    @id @default(cuid())
  createdAt DateTime? @default(now())
}

model Service {
  id              String  @id @default(cuid())
  title           String  @default("")
  details         String  @default("")
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_id        String?
}

model Skill {
  id    String        @id @default(cuid())
  type  SkillTypeType @default(dev)
  label String        @default("")
  value Int           @default(100)
}

model Education {
  id          String   @id @default(cuid())
  title       String   @default("")
  passed      DateTime
  certificate String   @default("")
  description Json     @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

enum SkillTypeType {
  lang
  dev
}